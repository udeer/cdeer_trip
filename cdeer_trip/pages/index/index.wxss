/**index.wxss**/
.page.iphone-x {
  padding-bottom: 20rpx;
}

.banner {
  position: relative;
  width: 100vw;
  height: 540rpx;
  z-index: 0;
}

.banner-bg {
  position: absolute;
  width: 100vw;
  bottom: 0;
  left: 0;
}

.banner::after {
  content: "";
  display: block;
  height: 80rpx;
  width: 100vw;
  position: absolute;
  left: 0;
  bottom: 0;
  background-image: linear-gradient(180deg,rgba(255,255,255,0.00) 0%,#F9F9F9 100%);
  pointer-events: none;
  z-index: -1;
}

.banner-info {
  position: absolute;
  left: 0;
  right: 0;
  margin: 0 40rpx;
}

.banner-title {
  font-weight: bold;
  font-size: 58rpx;
  color: #000;
  line-height: 68rpx;
}

.banner-title_text {
  display: inline-block;
  vertical-align: middle;
}

.banner-avatar {
  position: relative;
  vertical-align: middle;
  float: right;
}

.avatar {
  width: 72rpx;
  height: 72rpx;
  border: 5rpx solid #fff;
  border-radius: 50%;
  background-size: cover;
}

.default-avatar {
  width: 72rpx;
  height: 72rpx;
  border: 5rpx solid #fff;
  border-radius: 50%;
  background-image: url(https://cloud-minapp-16269.cloud.ifanrusercontent.com/default-avator.jpg);
  background-size: cover;
}

.unread-message {
  display: block;
  position: absolute;
  right: 0;
  top: 0;
  pointer-events: none;
}

.banner-brief {
  font-size: 28rpx;
  color: #121212;
  line-height: 40rpx;
  margin-top: 20rpx;
  letter-spacing: 4rpx;
}

.index-search {
  position: absolute;
  bottom: 10rpx;
  left: 40rpx;
  right: 40rpx;
  border-radius: 44rpx;
  box-shadow: 0 4rpx 20rpx 2rpx #FBEBD4;
}


.index {
  padding: 20rpx 36rpx;
}

.top_text {
  width: 100%;
  text-align: center;
  color: #AAA;
  font-size: 24rpx;
}

page::-webkit-scrollbar {
  display: none;
}

.slide_box {
  width: 678rpx;
  height: 280rpx;
  margin: 0 auto;
  box-shadow: 0rpx 2rpx 14rpx 0rpx rgba(38,38,38,0.1);
}

.slide_image {
  width: 678rpx;
  height: 280rpx;
  border-radius: 4rpx;
}

.dots {
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin-top: 20rpx;
}

.dot {
  width: 20rpx;
  height: 4rpx;
  background: rgba(221,221,221,1);
  margin-right: 20rpx;
}

.active {
  width: 60rpx;
  height: 4rpx;
  background: rgba(38,38,38,1);
}

.category {
  padding: 30rpx 10rpx;
}

.index_label {
  position: relative;
  margin-top: 80rpx;
}

.index_label_title {
  font-size: 48rpx;
  color: #262626;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}

.index_label_bg {
  width: 160rpx;
  height: 12rpx;
  background: linear-gradient(270deg,rgba(249,228,135,0) 0%,rgba(241,197,79,1) 100%);
