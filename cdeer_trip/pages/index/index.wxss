}

.index_article_during {
  position: absolute;
  top: 326rpx;
  left: 16rpx;
  z-index: 1;
  /* width: 120rpx; */
  height: 40rpx;
  background: rgba(38,38,38,1);
  border-radius: 4rpx;
  opacity: 0.9;
  padding:2rpx 10rpx;
