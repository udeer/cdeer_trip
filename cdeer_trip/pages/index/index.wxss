}

.index-search {
  display: flex;
  flex-flow: row;
  background: #FFFFFF;
  box-shadow: 0 6rpx 8rpx 0 #F5F5F5;
  border-radius: 22rpx;
  height: 88rpx;
  justify-content: center;
  align-items: center;
  padding: 24rpx;
  box-sizing: border-box;
}

.search-icon {
  flex-grow: 0;
  flex-shrink: 0;
