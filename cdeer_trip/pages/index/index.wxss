}

.margin_right_30 {
  margin-right: 30rpx;
}

.margin_right_0 {
  margin-right: 0;
}

.index_article_cover {
  width: 678rpx;
  height: 380rpx;
  border-radius: 4rpx;
}

.index_article {
  margin-top: 30rpx;
}

.index_article_title {
  font-size: 36rpx;
  font-weight: 400;
  color: rgba(38,38,38,1);
  line-height: 50rpx;
  margin-top: 30rpx;
  width: 678rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.index_article_desc {
  color: #888888;
  font-size: 28rpx;
  margin-bottom: 70rpx;
